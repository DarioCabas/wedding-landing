---
import CardVideo from "./CardVideo";
---

<section
  class="landing-section bg-gray-50 h-screen w-screen text-center overflow-hidden relative"
  id="about"
>
  <div
    class="items-start
  bg-gray-100
  self-stretch
  flex
  w-full
  h-full
  flex-col
  px-5
  py-10
  max-md:max-w-full"
  >
    <div
      class="items-center
    self-center
    flex
    w-full
    max-w-[1040px]
    flex-col
    mt-10
    max-md:max-w-full"
    >
      <h6
        class="self-center
      text-stone-600 text-center text-3xl
      font-bold
      leading-[125%]
      tracking-tighter
      w-[1040px]
      max-md:max-w-full"
      >
        Conocenos
      </h6>
      <h7
        class="self-center
      text-neutral-400 text-center text-base
      leading-[150%]
      w-[1040px]
      mt-1
      max-md:max-w-full"
      >
        Lo recordaras por siempre
      </h7>
    </div>
    <CardVideo  />
  </div>
</section>
