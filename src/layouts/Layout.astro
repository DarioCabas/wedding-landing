---
import LandingHeader from "../components/LandingHeader.astro";
interface Props {
  title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
  <meta>
    <meta charset="UTF-8" />
    <meta name="description" content="En quinta la Victoria nos complace atender a todas las personas y que obtengan un recuerdo inolvidable , ademas de inmortalizar todos los momentos que se vivan en nuestro hermoso lugar." />
    <meta name="viewport" content="width=device-width" />
    <link rel="shortcut icon" type="image/x-icon" href="/favicon.ico"/>
    <!-- OpenGraph -->
    <meta property="og:image" content="/portada3.jpg">
    <meta property="og:image:alt" content="Una fotografÃ­a de La Victoria" >
    <!-- <link rel="icon" type="image/svg+xml" href="/favicon.svg" /> -->
    
    <meta name="generator" content={Astro.generator} />
    <title>{title}</title>
    <meta name="robots" content="index, follow">
    <meta name="theme-color" content="#000000">

  </head>
  <body>
    
    <slot />
    <script
      is:inline
      src="https://cdnjs.cloudflare.com/ajax/libs/flowbite/1.8.0/flowbite.min.js"
    ></script>
  </body>
</html>

<style is:global>
  @font-face {
    font-family: "Georgia";
    src: url("/fonts/Georgia.ttf") format("truetype");
    font-style: normal;
    font-display: swap;
    font-weight: normal;

  }
</style>
